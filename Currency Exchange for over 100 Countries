int main() {
    initializeRates();
    cout << "Currency Converter (Choose from over 100 currencies)\n";
    cout << "Available currencies:\n";
    for (size_t i = 0; i < currencies.size(); ++i) {
        cout << (i + 1) << ". " << currencies[i].first << " - " << currencies[i].second << endl;
    }

    cout << "\nEnter amount: ";
    double amount;
    cin >> amount;

    cout << "Choose 'from' currency number: ";
    int fromIndex;
    cin >> fromIndex;
    if (fromIndex < 1 || fromIndex > currencies.size()) {
        cout << "Invalid choice.\n";
        return 1;
    }
    string from = currencies[fromIndex - 1].first;

    cout << "Choose 'to' currency number: ";
    int toIndex;
    cin >> toIndex;
    if (toIndex < 1 || toIndex > currencies.size()) {
        cout << "Invalid choice.\n";
        return 1;
    }
    string to = currencies[toIndex - 1].first;

    // Convert
    double usdAmount = amount / rates[from];
    double converted = usdAmount * rates[to];

    cout << fixed << setprecision(2);
    cout << amount << " " << from << " = " << converted << " " << to << endl;

    return 0;
}
